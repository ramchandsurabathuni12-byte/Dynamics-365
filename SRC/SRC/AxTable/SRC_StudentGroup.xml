			<Method>
				<Name>Exist</Name>
				<Source><![CDATA[
    public static boolean Exist(SRC_StudentId  _studentId)
    {
        SRC_StudentGroup studentGroup;
        if(_studentId !='')
        {
            select firstonly RecId
                from studentGroup
                where studentGroup.studentId == _studentId;
        }
        return (studentGroup.RecId != 0);
    }

]]></Source>
