				<Name>Find</Name>
				<Source><![CDATA[
    /// <summary>
    ///
    /// </summary>
    public static SRC_StudentGroup Find(SRC_StudentId _studentId,boolean _forUpdate =false)
    {
        SRC_StudentGroup studentGroup;
        if(_studentId !='')
        {
            studentGroup.selectForUpdate(_forUpdate);
            select firstonly studentGroup 
                where studentGroup.StudentId == _studentId;
        }
        return studentGroup;
    }

]]></Source>
