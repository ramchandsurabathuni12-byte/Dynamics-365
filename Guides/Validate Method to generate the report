public boolean validate()
{
boolean ret = true;
TransDate TodayDate = DateTimeUtil::getToday(DateTimeUtil::getUserPreferredTimeZone());
NSI_DailyAggregatedPostedDataHeader header = NSI_DailyAggregatedPostedDataHeader::findByTransDate(todayDate);

if(header.RecId && (header.IntegrationStatus == NSIIntegrationstatus::ReadyForAFAH ||header.IntegrationStatus == NSIIntegrationStatus::ReadyForValidation))
{
ret = checkFailed("label");
}
return ret;
}


